a:int, b:int -> sum:int -> {
    return a + b
}

n:int -> fib:int -> {
    if n == 0 {
        return 1
    }

    if n == 1 {
        return 1
    }

    return fib(n - 1) + fib(n - 2)
}

a:float, b:int -> power:float -> {
    i = 1
    c = a
    while i < b {
        c = c * a
        i = i + 1
    }
    return c
}

a = sum(6, 4) + power(2.0, 3)

output(a)

